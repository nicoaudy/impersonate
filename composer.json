{
    "name": "nicoaudy/impersonate",
    "description": "Laravel impersonate middleware, simply traits for impersonating other user",
    "license": "MIT",
    "authors": [
        {
            "name": "NicoAudy",
            "email": "nicoaudy1994@gmail.com",
            "homepage": "https://nicoaudy.github.io"
        }
    ],
    "homepage": "https://github.com/nicoaudy/impersonate",
    "keywords": ["Laravel", "Impersonate", "Package"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NicoAudy\\Impersonate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NicoAudy\\Impersonate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NicoAudy\\Impersonate\\ImpersonateServiceProvider"
            ],
            "aliases": {
                "Impersonate": "NicoAudy\\Impersonate\\Facades\\Impersonate"
            }
        }
    }
}
